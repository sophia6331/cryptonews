jQuery(function(e){"use strict";if(getbowtied_scripts_vars.option_minicart==1&&getbowtied_scripts_vars.option_minicart_open==1){e("body").on("click",".shopping-bag-button .tools_button",function(o){if(e("body").hasClass("gbt_custom_notif")){e(".gbt-custom-notification-notice").removeClass("open-notice").removeAttr("style").addClass("fade-out-notice")}if(e(window).width()>=1024){o.preventDefault();e(".shopkeeper-mini-cart").toggleClass("open");o.stopPropagation()}else{o.stopPropagation()}});e("body").on("click",function(o){if(e(".shopkeeper-mini-cart").hasClass("open")){if(!e(o.target).is(".shopkeeper-mini-cart")&&!e(o.target).is(".shopping-bags-button .tools-button")&&!e(o.target).is(".woocommerce-message")&&e(".shopkeeper-mini-cart").has(o.target).length===0){e(".shopkeeper-mini-cart").removeClass("open")}}})}if(e(window).width()>=1024){if(getbowtied_scripts_vars.option_minicart==1&&getbowtied_scripts_vars.option_minicart_open==2){e(".shopping-bag-button").on({mouseenter:function(o){if(!e(".shopkeeper-mini-cart").hasClass("open")){window.setTimeout(function(){if(e("body").hasClass("gbt_custom_notif")){e(".gbt-custom-notification-notice").removeClass("open-notice").removeAttr("style").addClass("fade-out-notice")}o.preventDefault();e(".shopkeeper-mini-cart").addClass("open");o.stopPropagation()},350)}},mouseleave:function(){window.setTimeout(function(){if(e(".shopkeeper-mini-cart").hasClass("open")&&!e(".shopkeeper-mini-cart").is(":hover")){e(".shopkeeper-mini-cart").removeClass("open")}},500)}});e(".shopkeeper-mini-cart").on({mouseleave:function(o){window.setTimeout(function(){if(e(".shopkeeper-mini-cart").hasClass("open")){e(".shopkeeper-mini-cart").removeClass("open")}},500)}})}}});