jQuery(function(e){"use strict";e(".woocommerce-tabs .row + .panel").addClass("current");e(".woocommerce-tabs ul.tabs li a").off("click").on("click",function(){var a=e(this);var s=a.attr("href");a.parent().siblings().removeClass("active").end().addClass("active");if(e(".woocommerce-tabs").find(s).siblings(".panel").filter(":visible").length>0){e(".woocommerce-tabs").find(s).siblings(".panel").filter(":visible").fadeOut(500,function(){e(".woocommerce-tabs").find(s).siblings(".panel").removeClass("current");e(".woocommerce-tabs").find(s).addClass("current").fadeIn(500)})}else{e(".woocommerce-tabs").find(s).siblings(".panel").removeClass("current");e(".woocommerce-tabs").find(s).addClass("current").fadeIn(500)}return false});e(".woocommerce-review-link").off("click").on("click",function(){e(".tabs li a").each(function(){if(e(this).attr("href")=="#tab-reviews"){e(this).trigger("click")}});var a=0;if(e("body").hasClass("admin-bar")){a+=32}var s=e(".woocommerce-tabs").offset().top-a;e("html, body").animate({scrollTop:s},1e3);return false})});