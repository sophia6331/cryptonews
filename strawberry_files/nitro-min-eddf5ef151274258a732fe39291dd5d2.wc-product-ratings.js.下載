jQuery(function(e){"use strict";if(e(window).width()>1024){var o=e(".product .woocommerce-product-rating .woocommerce-review-link").html();var r=e(".product .woocommerce_review_link_hover");var c=e(".product .woocommerce-product-rating");var i='<div class="woocommerce_review_link_hover">'+o+"</div>";if(o!=undefined){c.before(i);c.on({mouseenter:function(){e(".woocommerce_review_link_hover").addClass("hovered")},mouseleave:function(){e(".woocommerce_review_link_hover").removeClass("hovered")}})}}});