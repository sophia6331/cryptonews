jQuery(function(t){"use strict";var i=function(){var t=0;return function(){return t++}}();t(".wp-block-gallery").each(function(){var r=t(this);r.attr("id","gallery-"+i());r.find(".blocks-gallery-item").each(function(){var i=t(this);var a=i.find("a").attr("href");if(a){a=a.split(".").pop();if(a&&typeof a==="string"&&(a=="jpg"||a=="jpeg"||a=="png"||a=="gif")){i.find("a").addClass("fresco");i.find(".fresco").attr("data-fresco-group",r.attr("id"));if(i.find("figcaption").length>0){i.find(".fresco").attr("data-fresco-caption",i.find("figcaption").text())}}}})})});