jQuery(function(e){"use strict";function i(e){var i=document.cookie;var n=e+"=";var t=i.indexOf("; "+n);if(t==-1){t=i.indexOf(n);if(t!=0)return null}else{t+=2;var o=document.cookie.indexOf(";",t);if(o==-1){o=i.length}}return decodeURIComponent(decodeURIComponent(i.substring(t+n.length,o)))}function n(i){if(typeof i==="number"&&isFinite(i)&&Math.floor(i)===i&&i>=0){e(".wishlist_items_number").html(i)}}if(e(".wishlist_items_number").length){var t=0;var o=i("yith_wcwl_products");if(o!=null){o=o.slice(0,o.indexOf("]")+1);var r=JSON.parse(o);t=Object.keys(r).length}else{t=Number(e(".wishlist_items_number").html())}e("body").on("added_to_wishlist",function(){t++;n(t)});e("body").on("removed_from_wishlist",function(){t--;n(t)});n(t)}});